{
    "name": "delfimov/nanoframework",
    "license": "MIT",
    "type": "library",
    "description": "Quintessential PHP framework based on PSR and RFC standards",
    "keywords": ["PHP", "framework", "PSR", "RFC", "fast", "simple"],
    "homepage": "https://github.com/delfimov/nanoframework/",
    "authors": [
        {
            "name": "Dmitry Elfimov",
            "email": "elfimov@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nanoframework\\": "./src"
        }
    },
    "prefer-stable" : true,
    "require": {
        "php": ">=5.5.9",
        "psr/http-message": "^1.0",
        "guzzlehttp/psr7": "^1",
        "nikic/fast-route": "^1",
        "symfony/cache": "3.3.*",
        "monolog/monolog": "^1",
        "psr/container": "^1",
        "level-2/dice": "^2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "4.*"
    },
    "scripts" : {
        "phpcs": [
            "php ./vendor/bin/phpcs"
        ],
        "test": [
            "php ./vendor/bin/phpunit"
        ]
    }
}
